{
    "manifestVersion": 1,
    "id": "terraform-extension",
    "publisher": "arkiaconsulting",
    "version": "1.4.3",
    "name": "Terraforming (by AKC)",
    "description": "Tooling written in Node.js to easy your deployments using terraform (cross platform)",
    "public": true,
    "targets": [{
        "id": "Microsoft.VisualStudio.Services"
    }],
    "categories": [
        "Build and release"
    ],
    "tags": [
        "terraform",
        "azure",
        "release",
        "build",
        "azurerm",
        "infrastructure as code",
        "iac"
    ],
    "icons": {
        "default": "extension-icon.png"
    },
    "screenshots": [{
            "path": "screenshot0.png"
        },
        {
            "path": "screenshot1.png"
        },
        {
            "path": "screenshot2.png"
        },
        {
            "path": "screenshot3.png"
        },
        {
            "path": "screenshot4.png"
        }
    ],
    "content": {

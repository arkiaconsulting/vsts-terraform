{
    "manifestVersion": 1,
    "id": "terraform-extension",
    "publisher": "arkiaconsulting",
    "version": "1.0.20",
    "name": "Terraforming (by AKC)",
    "description": "Tooling written in Node.js to easy your deployments using terraform.",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "categories": [
        "Build and release"
    ],
    "tags": [
        "terraform",
        "azure",
        "release",
        "build",
        "azurerm",
        "infrastructure as code"
    ],
    "icons": {
        "default": "extension-icon.png"
    },
    "screenshots": [
        {
            "path": "screenshot.png"
        }
    ],
    "content": {
        "details": {
            "path": "README.md"
        }
    },
    "links": {
        "getstarted": {
            "uri": "https://github.com/arkiaconsulting/vsts-terraform"
        },
        "support": {
            "uri": "https://github.com/arkiaconsulting/vsts-terraform"
        }
    },
    "contributions": [
        {
            "id": "TerraformAkc",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "terraform"
            }
        }
    ],
    "files": [
        {
            "path": "terraform/src"
        }
    ]
}
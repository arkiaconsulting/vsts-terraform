{
    "manifestVersion": 1,
    "id": "terraform-extension",
    "publisher": "arkiaconsulting",
    "version": "1.0.10",
    "name": "Terraforming (by AKC)",
    "description": "A set of Terraform tasks",
    "public": false,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "tags": [
        "terraform",
        "azure",
        "release",
        "build",
        "azurerm",
        "infrastructure as code"
    ],
    "icons": {
        "default": "extension-icon.png"
    },
    "screenshots": [
        {
            "path": "screenshot.png"
        }
    ],
    "content": {
        "details": {
            "path": "README.md"
        }
    },
    "links": {
        "getstarted": {
            "uri": "https://github.com/arkiaconsulting/vsts-terraform"
        },
        "support": {
            "uri": "https://github.com/arkiaconsulting/vsts-terraform"
        }
    },
    "contributions": [
        {
            "id": "TerraformAkc",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "terraform"
            }
        }
    ],
    "files": [
        {
            "path": "terraform/src"
        }
    ]
}
{
    "manifestVersion": 1,
    "id": "terraform-extension",
    "publisher": "arkiaconsulting",
    "version": "1.2.1",
    "name": "Terraforming (by AKC)",
    "description": "Tooling written in Node.js to easy your deployments using terraform (cross platform)",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "categories": [
        "Build and release"
    ],
    "tags": [
        "terraform",
        "azure",
        "release",
        "build",
        "azurerm",
        "infrastructure as code"
    ],
    "icons": {
        "default": "extension-icon.png"
    },
    "screenshots": [
        {
            "path": "screenshot.png"
        }
    ],
    "content": {
        "details": {
            "path": "README.md"
        }
    },
    "links": {
        "getstarted": {
            "uri": "https://github.com/arkiaconsulting/vsts-terraform"
        },
        "support": {
            "uri": "https://github.com/arkiaconsulting/vsts-terraform"
        }
    },
    "contributions": [
        {
            "id": "TerraformAkc",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "terraform"
            }
        },
        {
            "id": "TerraformDownload",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "v2/terraformdownload"
            }
        },
        {
            "id": "TerraformInit",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "v2/terraforminit"
            }
        },
        {
            "id": "TerraformPlan",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "v2/terraformplan"
            }
        },
        {
            "id": "TerraformApply",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "v2/terraformapply"
            }
        },
        {
            "id": "TerraformOutput",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "v2/terraformoutput"
            }
        }
    ],
    "files": [
        {
            "path": "terraform/src"
        },
        {
            "path": "v2/terraformdownload"
        },
        {
            "path": "v2/terraforminit"
        },
        {
            "path": "v2/terraformplan"
        },
        {
            "path": "v2/terraformapply"
        },
        {
            "path": "v2/terraformoutput"
        }
    ]
}
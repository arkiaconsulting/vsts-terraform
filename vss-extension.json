            "properties": {
                "name": "v2/terraformworkspace"
            }
        },
        {
            "id": "TerraformDestroy",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "v2/terraformdestroy"
            }
